[*.cs]

# IDE0058: O valor da expressão nunca é usado
dotnet_diagnostic.IDE0058.severity = none

# CS8629: O tipo de valor de nulidade pode ser nulo.
dotnet_diagnostic.CS8629.severity = silent

# CS0618: O tipo ou membro é obsoleto
dotnet_diagnostic.CS0618.severity = silent

# CS8618: O campo não anulável precisa conter um valor não nulo ao sair do construtor. Considere adicionar o modificador "obrigatório" ou declarar como anulável.
dotnet_diagnostic.CS8618.severity = silent

# CS8604: Possível argumento de referência nula.
dotnet_diagnostic.CS8604.severity = silent
